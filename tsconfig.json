{
	"compilerOptions": {
		"noEmit": true,
		"module": "esnext",
		"target": "esnext",
		"lib": [
			"esnext"
		],
		"strict": true,
		"moduleResolution": "node",
		"types": [
			"@cloudflare/workers-types"
		],
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true
	},
	"exclude": [
		"node_modules",
		"dist",
		"test"
	]
}